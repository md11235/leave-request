        </extensionElements>
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApproved > 0}]]></conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="flow18" sourceRef="exclusiveGateway3" targetRef="errorEndEvent3">
        <extensionElements>
          <flowable:executionListener event="end" expression="${requestListener.onReject(execution)}"></flowable:executionListener>
        </extensionElements>
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isApproved < 0}]]></conditionExpression>
      </sequenceFlow>
    </subProcess>
    
    <boundaryEvent id="boundaryError3" name="Error" attachedToRef="constructionCompanyReviewSubProcess">
      <errorEventDefinition errorRef="rejected"></errorEventDefinition>
    </boundaryEvent>
    
    <sequenceFlow id="flow14" sourceRef="enquiryReviewSubProcess" targetRef="constructionCompanyReviewSubProcess"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="boundaryerror1" targetRef="submitRequestTask"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="boundaryerror2" targetRef="costControlReviewSubProcess"></sequenceFlow>
    <sequenceFlow id="flow19" sourceRef="boundaryError3" targetRef="enquiryReviewSubProcess"></sequenceFlow>
    <sequenceFlow id="flow20" sourceRef="constructionCompanyReviewSubProcess" targetRef="endevent4"></sequenceFlow>

    <endEvent id="endevent4" name="End"></endEvent>
    
