        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApproved < 0}]]></conditionExpression>
      </sequenceFlow>
    </subProcess>
    
    <boundaryEvent id="boundaryerror2" name="Error" attachedToRef="enquiryReviewSubProcess">
      <errorEventDefinition errorRef="rejected"></errorEventDefinition>
    </boundaryEvent>
    
    <subProcess id="constructionCompanyReviewSubProcess" name="Construct Company">
